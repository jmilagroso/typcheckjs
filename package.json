  "bugs": {
    "url": "https://github.com/jmilagroso/typcheckjs/issues"
  },
  "homepage": "https://github.com/jmilagroso/typcheckjs#readme",
  "devDependencies": {
    "coveralls": "^3.0.4",
    "eslint": "^9.19.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.17.2",
    "husky": "^9.0.11",
    "jest": "^29.3.1"
  }
